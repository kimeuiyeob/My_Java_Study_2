package ∂˜¥ŸΩƒ;

interface C {
	int abc(String str);
}

public class RefOfStaticMethod_Type2_2 {
	public static void main(String[] args) {

		C c = a -> {
			if (a.equals("æ»≥Á")) {
				return 1;
			} else {
				return 2;
			}
		};
		int number = c.abc("æ»≥Á");
		System.out.println(number);
		
//		==============================
		
		C c1 = a -> a.length();
		System.out.println(c1.abc("æ»≥Á«œººø‰"));
	
		
		
	}
}
